[
  {
    "kernelSource": {
      "name": "a71",
      "repo": "https://github.com/hung319/a71-k",
      "branch": "main",
      "device": "a71",
      "defconfig": "a71_eur_open_defconfig"
    },
    "toolchains": [
      {
        "name": "llvm-arm-toolchain-ship-10.0",
        "repo": "https://github.com/proprietary-stuff/llvm-arm-toolchain-ship-10.0",
        "branch": "master",
        "binaryEnv": ["./bin"]
      },
      {
        "name": "android_prebuilts_gcc_linux-x86_aarch64_aarch64-linux-android-4.9",
        "url": "https://github.com/LineageOS/android_prebuilts_gcc_linux-x86_aarch64_aarch64-linux-android-4.9",
        "binaryEnv": ["./bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CROSS_COMPILE": "llvm-arm-toolchain-ship-10.0/bin/aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "llvm-arm-toolchain-ship-10.0/bin/armv7-linux-androideabi",
        "CLANG_TRIPLE": "aarch64-linux-gnu-"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "custom": {
        "repo": "https://github.com/CCCC-L/anyKernel3",
        "branch": "main"
      }
    },
    "enableKernelSU": true,
    "enableLXC": false
  }
]
