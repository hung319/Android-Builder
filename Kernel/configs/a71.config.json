[
  {
    "kernelSource": {
      "name": "android_kernel_samsung_sm7150",
      "repo": "https://github.com/pascua28/android_kernel_samsung_sm7150",
      "branch": "prime++",
      "device": "a71",
      "defconfig": "a71_eur_open_defconfig"
    },
    "toolchains": [
      {
        "name": "clang",
        "repo": "https://github.com/ravindu644/Toolchains_by_Google.git",
        "branch": "main",
        "binaryEnv": ["./clang-10.0/bin"]
      },
      {
        "name": "gcc",
        "repo": "https://github.com/ravindu644/Toolchains_by_Google.git",
        "branch": "main",
        "binaryEnv": ["./aarch64-4.9/bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-gnueabi-",
        "CLANG_TRIPLE": "aarch64-linux-gnu-"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": true,
    "enableLXC": false
  }
]
